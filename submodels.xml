  <!-- #   Guns,  .50 x 1                                # -->
  <!-- #                                                 # -->
  <!-- ################################################### -->


  <submodel>
    <name>left-guns-tracers</name>
    <model>Aircraft/m113/Stores/guns/tracer.xml</model>
    <trigger>controls/armament/trigger</trigger>
    <speed>3000.0</speed>
    <repeat>true</repeat>
    <delay>1.00</delay>
    <count>-1</count>
    <x-offset> 0.0</x-offset>
    <y-offset>-1.20</y-offset>
    <z-offset>6.40</z-offset>
    <yaw-offset>0.0</yaw-offset>
    <pitch-offset>0.0</pitch-offset>
    <life>3.0</life>
    <wind>false</wind>
    <weight>0.2</weight>
    <cd>0.2</cd>
    <eda>0.00136354</eda> 
  </submodel>

  <submodel>
    <name>left-guns-bullets</name>
    <model>Models/Geometry/null.ac</model>
    <trigger>controls/armament/trigger</trigger>
    <speed>3000.0</speed>
    <repeat>true</repeat>
    <delay>0.00</delay>
    <count>-1</count>
    <x-offset> 0.0</x-offset>
    <y-offset>-1.20</y-offset>
    <z-offset>6.40</z-offset>
    <yaw-offset>0.0</yaw-offset>
    <pitch-offset>0</pitch-offset>
    <life>3.0</life>
    <wind>false</wind>
    <weight>0.22</weight>
    <cd>0.2</cd>
    <eda>0.00136354</eda> 
    <collision>true</collision>
    <collision-report>sim/ai/aircraft/collision/bullet</collision-report>
    <impact>true</impact>
    <impact-report>sim/ai/aircraft/impact/bullet</impact-report>
    <submodel-path>Aircraft/m113/Stores/guns/bullet-submodel.xml</submodel-path>
  </submodel>


